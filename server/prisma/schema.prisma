generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// data_ora DATETIME = 'YYYY-MM-DD HH:MM:SS'
model createPost {
  id int @id @default(autoincrement());
  data_ora DATETIME;
  typeOfPost string;
  appType string;
  idImage? int;
  idAccount? string;
}

model ImageStorage{
  id int @id;
  url string;
}

model posts {
  id int @id
  data_ora DATETIME
  typeOfPost string;
  addType string;
  appType string;
}

